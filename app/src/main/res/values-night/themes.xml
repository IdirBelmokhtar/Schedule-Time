<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.AppCompat.background_" parent="">
        <item name="android:background">@color/dark_</item>
    </style>
    <style name="Theme.AppCompat.background" parent="">
        <item name="android:background">@color/dark</item>
    </style>

    <!-- Base application theme. -->
    <style name="Theme.ScheduleTime" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:colorEdgeEffect">@color/transparent</item>
    </style>

    <style name="Theme.ScheduleTimes" parent="Theme.MaterialComponents">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue</item>
        <item name="colorSecondaryVariant">@color/blue</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:colorEdgeEffect">@color/transparent</item>
    </style>

    <style name="Theme.Toolbar.backgroundColor" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:background">@color/blue</item>
    </style>

    <style name="ExpandedAppBar" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#000</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="CollapsedAppBar" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="Theme.ScheduleTime.LinearLayout.ListItem" parent="">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="Theme_ScheduleTime_LinearLayout_ListItem_Clickable" parent="Theme.ScheduleTime.LinearLayout.ListItem">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_dark_mode</item>
    </style>

    <style name="Theme.ScheduleTime.LinearLayout.ListItem.CircleClickable" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_circle_dark_mode</item>
    </style>

    <style name="Theme.ScheduleTime.LinearLayout.ListItem.CircleClickable2" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_dark_mode</item>
    </style>

    <style name="Theme.ScheduleTime.LinearLayout.ListItem.CircleClickable3" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_circle_white</item>
    </style>

    <style name="Theme_ScheduleTime_LinearLayout_ListItem_CircleClickable4" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_circle_white_dark_mode</item>
    </style>

    <style name="Theme_ScheduleTime_LinearLayout_ListItem_CircleClickable4_selection" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_circle_white_dark_mode_selection</item>
    </style>

    <style name="Theme.ScheduleTime.LinearLayout.ListItem_" parent="">
        <item name="android:clickable">false</item>
        <item name="android:background">@color/dark</item>
    </style>

    <style name="Widget.Schedule.TextView" parent="Widget.MaterialComponents.TextView">
        <item name="android:textAppearance">?attr/textAppearanceBodyLarge</item>
        <item name="android:lineSpacingExtra">-1.5sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textAlignment">viewStart</item>
        <item name="android:breakStrategy" tools:targetApi="m">high_quality</item>
        <item name="android:hyphenationFrequency" tools:targetApi="m">full</item>
    </style>

    <style name="TextInputLineColor" >
        <!-- Hint color and label color in FALSE state
        <item name="android:textColorHint">@color/Color Name</item>
        <item name="android:textSize">20sp</item> -->
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
        <item name="colorControlNormal">@color/blue_2</item>
        <item name="colorControlActivated">@color/blue_2</item>
        <item name="colorControlHighlight">@color/blue_2</item>
    </style>

    <style name="Theme.AppCompat.EditText.textColor.textColorHint" parent="">
        <item name="android:textColorHint">@color/_white</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Theme.AppCompat.TextInputLayout">
        <item name="android:textColorHint">@color/white_</item>
        <item name="endIconTint">@color/white_</item>
        <item name="hintTextColor">@color/white_</item>
    </style>

    <style name="Theme.AppCompat.TextInputLayout.Colors" parent="">
        <item name="android:textColorHint">@color/white_</item>
        <item name="endIconTint">@color/white_</item>
        <item name="hintTextColor">@color/white_</item>
        <item name="android:textColor">@color/white_</item>
    </style>

    <style name="Theme.AppCompat.custom_background" parent="">
        <item name="android:background">@drawable/custom_background_dark</item>
    </style>

    <style name="Theme.AppCompat.custom_background_" parent="">
        <item name="android:background">@drawable/custom_background_dark_</item>
    </style>

    <style name="Theme.AppCompat.View" parent="">
        <item name="android:background">@color/blue</item>
    </style>

    <style name="Theme.AppCompat.custom_button_stroke_black_search" parent="">
        <item name="android:background">@drawable/custom_button_stroke_black_search_dark_mode</item>
    </style>

    <style name="AppTheme.CardView.backgroundColor" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:backgroundTint">@color/blue_</item>
    </style>

    <style name="ThemeOverlay.AppCompat.navThemeDrawer">
        <!-- Color of text and icon when SELECTED -->
        <item name="colorPrimary">@color/white</item>
        <!-- Background color when SELECTED -->
        <item name="colorControlHighlight">@color/transparent</item>

        <item name="itemTextColor">@color/white_</item>
    </style>

    <style name="Theme.AppCompat.RadioButton" parent="">
        <item name="android:buttonTint">@color/white_</item>
        <item name="android:textColor">@color/white_</item>
    </style>

    <style name="Theme.AppCompat.CheckBox" parent="">
        <item name="android:buttonTint">@color/white_</item>
        <item name="android:textColor">@color/white_</item>
        <item name="android:textColorHighlight">@color/white_</item>
        <item name="android:textColorLink">@color/white_</item>
    </style>

    <style name="Theme.ScheduleTime.TextView.Clickable" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_text_dark_mode</item>
        <item name="android:textColor">@color/white_</item>
    </style>

    <style name="Theme.ScheduleTime.TextView.Clickable_" parent="">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/ripple_list_item_bg_text_dark_mode_</item>
        <item name="android:textColor">@color/white_</item>
    </style>

    <style name="SheetDialogAnimation" >
        <item name="android:windowEnterAnimation">@anim/slide_in_y</item>
        <item name="android:windowExitAnimation">@anim/slide_out_y</item>
    </style>

    <style name="Color1SwitchStyle">
        <item name="colorControlActivated">@color/blue_2</item>
    </style>

    <style name="Theme.AppCompat.calendarView" parent="">
        <item name="android:background">@color/dark</item>
    </style>

    <style name="Theme.AppCompat.Textview" parent="">
        <item name="android:textColor">@color/white_</item>
    </style>

    <style name="Theme.AppCompat.Textview_" parent="">
        <item name="android:textColor">@color/_white</item>
    </style>

    <style name="Theme.AppCompat._Textview" parent="">
        <item name="android:textColor">@color/_white</item>
    </style>

    <style name="Theme.AppCompat.icon.edit" parent="">
        <item name="android:src">@drawable/ic_icons8_edit_2</item>
    </style>

    <style name="Theme.AppCompat.icon.delete" parent="">
        <item name="android:src">@drawable/icons8_delete_2</item>
    </style>

    <style name="Theme.AppCompat.icon.more" parent="">
        <item name="android:src">@drawable/icons8_more_2</item>
    </style>

    <style name="Theme.AppCompat.icon.trash" parent="">
        <item name="android:src">@drawable/icons8_trash_can_3</item>
    </style>

    <style name="WindowAnimationFadeInOut">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>
</resources>